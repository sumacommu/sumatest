{
  "version": 2,
  "builds": [
      { "src": "js/app.js", "use": "@vercel/node" },
      { "src": "*.html", "use": "@vercel/static" },
      { "src": "css/*", "use": "@vercel/static" }
  ],
  "routes": [
      { "src": "/auth/(.*)", "dest": "js/app.js" },
      { "src": "/(.*)", "dest": "/$1" }
  ]
}