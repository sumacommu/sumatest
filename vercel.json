{
  "version": 2,
  "builds": [
    { "src": "app.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/characters", "dest": "/characters/01.png" },
    { "src": "/auth/(.*)", "dest": "app.js" },
    { "src": "/logout", "dest": "app.js" },
    { "src": "/solo/(.*)", "dest": "app.js" },
    { "src": "/(.*)", "dest": "app.js" }
  ]
}